{#
/**
 * @file
 * Default theme implementation for an individual views swiper cycle slide.
 *
 * Available variables:
 * - classes: Classes to apply to the element.
 * - count: The slide number.
 * - rendered_items: Rendered items for slide.
 * - vss_id: The swiper's id.
 *
 * @see template_preprocess_lp_swiper_()
 *
 * @ingroup vss_templates
 */
#}
<div id="lp_swiper_cycle_div_{{ vss_id }}_{{ count }}" {{ attributes.addClass(classes) }}>
  {% for rendered_item in rendered_items %}
    {{ rendered_item }}
  {% endfor %}
</div>
